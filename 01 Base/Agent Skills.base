views:
  - type: table
    name: 表格
    filters:
      and:
        - file.tags.contains("agent-skills")
    order:
      - file.name
      - file.ctime
      - source
    sort:
      - property: source
        direction: DESC
  - type: cards
    name: 视图
    filters:
      and:
        - file.tags.contains("agent-skills")
    image: note.cover
    imageAspectRatio: 0.65
